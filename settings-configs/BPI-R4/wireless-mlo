config wifi-iface 'default_radio1_mld'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ocv '0'
	option mld_id '1'

config wifi-iface 'default_radio2_mld'
	option device 'radio2'
	option network 'lan'
	option mode 'ap'
	option ocv '0'
	option mld_id '1'
